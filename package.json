{
  "name": "@opensearch-project/oui",
  "description": "OpenSearch UI Component Library",
  "version": "2.0.0-alpha.2",
  "license": "Apache-2.0",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./style.css": {
      "default": "./dist/style.css"
    },
    "./style.scoped.css": {
      "default": "./dist/style.scoped.css"
    }
  },
  "files": [
    "dist/**/*"
  ],
  "engines": {
    "node": ">=20"
  },
  "docker_image": "node:10",
  "sideEffects": [
    "*.css",
    "*.scss"
  ],
  "scripts": {
    "build": "yarn run build:lib && yarn run build:storybook",
    "build:lib": "tsc --project tsconfig.build.json && tsc-alias && npx @tailwindcss/cli -i src/styles/preset.css -o dist/style.css && npx tsx scripts/scope-css.ts",
    "build:storybook": "storybook build -o ./docs",
    "build-docs": "yarn run build:storybook",
    "clean": "rm -rf build node_modules dist lib docs",
    "fix:shadcn": "npx tsx scripts/fix-shadcn-components.ts",
    "lint": "yarn run lint:ts && yarn run lint:es",
    "lint:es": "eslint --cache \"{src,scripts}/**/*.{ts,tsx,js}\" --max-warnings 0",
    "lint:es:fix": "yarn run lint:es --fix",
    "lint:ts": "tsc --project tsconfig.typecheck.json --noEmit",
    "lint:ts:watch": "tsc --project tsconfig.typecheck.json --noEmit --watch",
    "prebuild": "mkdir -p dist",
    "prepublishOnly": "yarn run build",
    "start": "storybook dev -p 8030 --no-open",
    "test": "yarn run test:storybook && yarn run test:eslint",
    "test:eslint": "node scripts/eslint-plugin/require_license_header.test.js && node scripts/eslint-plugin/i18n.test.js && node scripts/eslint-plugin/rel.test.js && node scripts/eslint-plugin/forward_ref_display_name.test.js",
    "test:storybook": "vitest --project=storybook --run"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/virajsanghvi/oui.git"
  },
  "resolutions": {
    "date-fns": "^2.3.0",
    "react-hook-form": "7.45.4",
    "cross-spawn": "^7.0.6"
  },
  "pre-commit": [
    "lint"
  ],
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-aspect-ratio": "^1.1.8",
    "@radix-ui/react-avatar": "^1.1.11",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-context-menu": "^2.2.16",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-hover-card": "^1.1.15",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-menubar": "^1.1.16",
    "@radix-ui/react-navigation-menu": "^1.2.14",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-progress": "^1.1.8",
    "@radix-ui/react-radio-group": "^1.3.8",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toggle": "^1.1.10",
    "@radix-ui/react-toggle-group": "^1.1.11",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@tailwindcss/cli": "^4.1.13",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "date-fns": "^2.3.0",
    "embla-carousel-react": "^8.6.0",
    "input-otp": "^1.4.2",
    "lucide-react": "^0.555.0",
    "next-themes": "^0.4.6",
    "react-day-picker": "^9.12.0",
    "react-hook-form": "7.45.4",
    "react-resizable-panels": "^3.0.6",
    "recharts": "^2.15.4",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1",
    "tw-animate-css": "^1.3.8",
    "vaul": "^1.1.2",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@storybook/addon-a11y": "^9.1.16",
    "@storybook/addon-docs": "^9.1.8",
    "@storybook/addon-links": "^9.1.16",
    "@storybook/addon-vitest": "^9.1.16",
    "@storybook/react": "^9.1.16",
    "@storybook/react-vite": "^9.1.16",
    "@storybook/test": "9.0.0-alpha.2",
    "@tailwindcss/postcss": "^4.1.13",
    "@testing-library/dom": "^10.0.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@tsconfig/node20": "^20.1.6",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@types/node": "^24.10.1",
    "@types/react": "^19.0.7",
    "@types/react-dom": "^19.0.3",
    "@types/react-18": "npm:@types/react@^18.3.17",
    "@types/react-dom-18": "npm:@types/react-dom@^18.3.5",
    "@vitest/browser": "^3.0.0",
    "@vitest/coverage-v8": "^3.0.0",
    "@vitest/ui": "^3.0.0",
    "autoprefixer": "^10.4.21",
    "dedent": "^0.7.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-webpack": "^0.13.9",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-local": "^1.0.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react-hooks": "^5.1.0",
    "glob": "^13.0.0",
    "jsdom": "^27.2.0",
    "playwright": "^1.57.0",
    "postcss": "^8.5.6",
    "pre-commit": "^1.2.2",
    "prettier": "^3.4.2",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-18": "npm:react@^18.3.1",
    "react-dom-18": "npm:react-dom@^18.3.1",
    "storybook": "^9.1.17",
    "tailwindcss": "^4.1.13",
    "tsc-alias": "^1.8.16",
    "typescript": "^5.9.2",
    "vite": "^7.2.6",
    "vitest": "^3.0.0"
  },
  "peerDependencies": {
    "@types/react": "*",
    "@types/react-dom": "*",
    "react": "^16.14 || ^17.0 || ^18.0 || ^19.0",
    "react-dom": "^16.14 || ^17.0 || ^18.0 || ^19.0"
  },
  "peerDependenciesMeta": {
    "@types/react": {
      "optional": true
    },
    "@types/react-dom": {
      "optional": true
    }
  },
  "packageManager": "yarn@1.22.22+sha1.ac34549e6aa8e7ead463a7407e1c7390f61a6610"
}
